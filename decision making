#greatest of two numbers
a=int(input("enter a number:"))
b=int(input("enter a number:"))
if a>=b:
    if a==b:
        print(a,"is equal to",b)
    else:
        print(a,"is greater than",b)
else:
    print(a,"is less than",b)
    
#vowels or consonants
c=input()
if c in('a','e','i','o','u'):
           print("Vowel")
elif c in ('b','c','d','f','g','h','j','k','l','m','n','p','q','r','s','t','v','w','x','y','z'):
    print("consonant")
else:
    print("Not an alphabet")

#grading system
g=int(input("enter your marks:"))
if g==100:
    print("s")
elif g>90 and g<100:
    print("A")
elif g>80 and g<90:
    print("B")
elif g>70 and g<80:
    print("C")
elif g>60 and g<70:
    print("D")
elif g>50 and g<60:
    print("E")
elif g<50:
    print("F")
else:
    print("invalid input")
    
#profit or loss
x=float(input())
y=int(input())
a=12*y
print("Enter the price of a dozen mangoes")
print("Enter the price at which 1 mango being sold")
if x>a:
  l=float(x-a)
  format_l="{:.2f}".format(l)
  print("Loss : Rs.",format_l)
elif x<a:
    p=float(a-x)
    format_p="{:.2f}".format(p)
    print("Profit : Rs.",format_p)
else:
    print("No profit nor loss")

#fee collection
stu=input("Enter the student type\n")
if(stu=="MSDS"):
    t=float(input("Enter tuition fee\n"))
    b=float(input("Enter bus fee\n"))
    tf=t+b
    print("The fees to be paid by the student is Rs.%.2f"% tf)
elif(stu=="MSH"):
    t=float(input("Enter tuition fee\n"))
    h=float(input("Enter hostel fee\n"))
    tf=t+h
    print("The fees to be paid by the student is Rs.%.2f"% tf)
elif(stu=="MGSD"):
    t=float(input("Enter tuition fee\n"))
    b=float(input("Enter bus fee\n"))
    p=(150/100)*t
    tf=p+b
    print("The fees to be paid by the student is Rs.%.2f"% tf)
elif(stu=="MGSH"):
    t=float(input("Enter tuition fee\n"))
    h=float(input("Enter hostel fee\n"))
    p=(150/100)*t
    tf=p+h
    print("The fees to be paid by the student is Rs.%.2f"% tf)
else:
    print("Invalid")
    
    #Y2k problem detector
    a=int(input("enter year of birth:"))
b=int(input("enter current year:"))
if(a%10!=a and b%10!=b):
       a=1900+a
       b=1900+b
else:
    a=1900+a
    b=2000+b
if a>b:
    age=a-b
else:
    age=b-a
if age>100:
    d=age-100
    print("your age is:",d)
else:
    print("your age is",age)
    

#lab allocation
x=int(input("enter x:"))
y=int(input("enter y:"))
z=int(input("enter z:"))
if x<y and x<z:
    print("L1 has the minimal seating capacity")
elif y<x and y<z:
    print("L2 has the minimal seating capacity")
else:
    print("L3 has the minimal seating capacity")
    
